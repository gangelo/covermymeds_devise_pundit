<%- model_class = User -%>
<div class="page-header">
  <h1><%= translate('.title',
            default: model_class.model_name.human.pluralize.titleize) %></h1>
</div>

<%= render partial: 'shared/user_search' %>

<%= render partial: 'shared/users', locals: { users: @search_criteria.users }%>

<div class='row'>
  <%= render partial: 'shared/pager', object: @search_criteria.users, as: :pager_data, locals: { css_class: 'col-lg-6 col-lg-offset-3 pager-center' } %>
</div>
